@model Onyx.Models.CustomerSettingModels

@{
    ViewBag.Title = "Detail Setting";
}

<h2>DetailSetting</h2>

@using (Html.BeginForm("DetailSettings", "Customer", FormMethod.Post))
{
    <table cellpadding="0" cellspacing="0">
        <tr>
            <th colspan="2" align="center">Customer Settings Details</th>
        </tr>
        <tr>
            <td>Buscar: </td>
            <td>
                @Html.TextBoxFor(m => m.KeyID)
            </td>
        </tr>
        <tr>
            <td></td>
            <td><input type="submit" value="Submit" /></td>
        </tr>
    </table>


}
