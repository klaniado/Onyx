@model IEnumerable <Onyx.Models.CustomerSettingModels>

@{
    ViewBag.Title = "DetailSettings";
}

<h2>DetailSettings</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CustomerSettingID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.KeyID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Value)
        </th>
        <th>
            @Html.DisplayText("Configuraciones")
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CustomerSettingID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.KeyID)
        </td>
        <td>
           @Html.DisplayFor(modelItem => item.Value)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new {id=item.CustomerSettingID}) |
            @Html.ActionLink("Details", "Detail", new { id=item.CustomerSettingID}) |
            @Html.ActionLink("Delete", "Delete", new { id=item.CustomerSettingID})
        </td>
    </tr>
}

</table>
